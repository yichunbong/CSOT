            <cdd:DataItem Name="IF_LOT" Category="MESFAB">
              <MetaInfo>
                <cdm:DataMetaInfo Name="IF_LOT">
                  <Properties>
                    <cdm:PropertyMeta Name="ORIENTEDSITE" PropertyType="string" />
                    <cdm:PropertyMeta Name="FACTORYNAME" PropertyType="string" />
                    <cdm:PropertyMeta Name="LOTNAME" PropertyType="string" />
                    <cdm:PropertyMeta Name="PROCESSGROUPNAME" PropertyType="string" />
                    <cdm:PropertyMeta Name="PROCESSFLOWNAME" PropertyType="string" />
                    <cdm:PropertyMeta Name="PRODUCTSPECNAME" PropertyType="string" />
                    <cdm:PropertyMeta Name="PRODUCTSPECVERSION" PropertyType="string" />
                    <cdm:PropertyMeta Name="PRODUCTIONTYPE" PropertyType="string" />
                    <cdm:PropertyMeta Name="PRIORITY" PropertyType="int" />
                    <cdm:PropertyMeta Name="PROCESSOPERATIONNAME" PropertyType="string" />
                    <cdm:PropertyMeta Name="CARRIERNAME" PropertyType="string" />
                    <cdm:PropertyMeta Name="PRODUCTTYPE" PropertyType="string" />
                    <cdm:PropertyMeta Name="LOTPROCESSSTATE" PropertyType="string" />
                    <cdm:PropertyMeta Name="LOTHOLDSTATE" PropertyType="string" />
                    <cdm:PropertyMeta Name="PRODUCTQUANTITY" PropertyType="int" />
                    <cdm:PropertyMeta Name="MACHINENAME" PropertyType="string" />
                    <cdm:PropertyMeta Name="RELEASETIME" PropertyType="DateTime" />
                    <cdm:PropertyMeta Name="LASTLOGGEDOUTTIME" PropertyType="DateTime" />
                    <cdm:PropertyMeta Name="LASTLOGGEDINTIME" PropertyType="DateTime" />
                    <cdm:PropertyMeta Name="LASTEVENTTIME" PropertyType="DateTime" />
                    <cdm:PropertyMeta Name="LOTSTATE" PropertyType="string" />
                    <cdm:PropertyMeta Name="OWNER_TYPE" PropertyType="string" />
                    <cdm:PropertyMeta Name="OWNER_ID" PropertyType="string" />
                    <cdm:PropertyMeta Name="HOLD_TIME" PropertyType="DateTime" />
                    <cdm:PropertyMeta Name="SHEETNAME" PropertyType="string" />
                    <cdm:PropertyMeta Name="SHEETNAME_TIME" PropertyType="DateTime" />
                    <cdm:PropertyMeta Name="INSP_SHEET_ID" PropertyType="string" />
                    <cdm:PropertyMeta Name="INSP_SHEET_TIME" PropertyType="DateTime" />
                    <cdm:PropertyMeta Name="STAY_HOURS" PropertyType="float" />
                    <cdm:PropertyMeta Name="Q_TIME" PropertyType="float" />
                    <cdm:PropertyMeta Name="NODEID" PropertyType="string" />
                    <cdm:PropertyMeta Name="PRE_PROCESS_ID" PropertyType="string" />
                    <cdm:PropertyMeta Name="PRE_STEP_ID" PropertyType="string" />
                    <cdm:PropertyMeta Name="ONMACHINE" PropertyType="string" />
                    <cdm:PropertyMeta Name="MACHINENAME_DURABLE" PropertyType="string" />
                  </Properties>
                </cdm:DataMetaInfo>
              </MetaInfo>
              <ActiveAction>
                <cdd:DataAction Name="Default" DataSource="T6MESFAB" IsAdapters="False" def:name="IF_LOT_Default">
                  <Commands>
                    <cdd:DataCommand Name="Cmd1" IsBindTable="True">
                      <CommandText>WITH WIP AS (
            SELECT * FROM LOT A
             WHERE 1=1
			   AND A.PROCESSOPERATIONNAME &lt;&gt; '0000' --BANK WIP 제외
               AND A.LOTSTATE = 'Released'    
               AND A.PRODUCTIONTYPE = 'Production'
               AND A.FACTORYNAME IN ($ARG_TARGET_SHOP_LIST)  
), INSP_SH AS (
    SELECT *
      FROM (
            SELECT A.*
              FROM CT_INCREASECONFIRMATIONSHEETLOT A, (SELECT LOTNAME, MAX(LASTEVENTTIMEKEY) AS LASTEVENTTIMEKEY
                                                         FROM CT_INCREASECONFIRMATIONSHEETLOT
                                                        GROUP BY  LOTNAME
                                                      ) B
             WHERE A.LOTNAME = B.LOTNAME
               AND A.LASTEVENTTIMEKEY = B.LASTEVENTTIMEKEY
    )
     WHERE INSPECTIONSTATE = 'OPEN'
), WIP_PROD AS (
        SELECT P.LOTNAME, P.PRODUCTNAME, DECODE(PF.PROCESSFLOWTYPE, 'Rework', C.MAINOPERATIONNAME , P.NEXTMAINPROCESSOPERATIONNAME) PRE_OPER, ROUND(MAXQUEUETIME - ((SYSDATE-FROMTRACKOUTTIME)*24*60),2) REMAINTIME 
          FROM PRODUCT P, PROCESSFLOW PF, CT_PRODUCTQUEUETIME A, CT_NEXTMAINOPERATION C
         WHERE 1=1
           AND P.PROCESSFLOWNAME = PF.PROCESSFLOWNAME
           AND P.PRODUCTNAME = A.PRODUCTNAME(+)
           AND P.PROCESSFLOWNAME = C.PROCESSFLOWNAME(+)
           AND P.PROCESSOPERATIONNAME = C.PROCESSOPERATIONNAME(+)
           AND P.FACTORYNAME IN ($ARG_TARGET_SHOP_LIST)
           AND P.LOTNAME IN (SELECT LOTNAME FROM  WIP)             
), LOT_QUE AS (
        SELECT LOTNAME, MIN(REMAINTIME) REMAINTIME
          FROM WIP_PROD
         GROUP BY LOTNAME
), WIP_QUE_NEXT_OPER AS (
        SELECT B.LOTNAME, MIN(PRE_OPER) PRE_OPER, MIN(B.REMAINTIME) REMAINTIME
          FROM LOT_QUE A, WIP_PROD B
         WHERE A.LOTNAME = B.LOTNAME
           AND A.REMAINTIME = B.REMAINTIME
         GROUP BY B.LOTNAME
 )
SELECT 
    A.ORIENTEDSITE,
    A.FACTORYNAME,
    A.LOTNAME,
    A.PROCESSGROUPNAME,
    A.PROCESSFLOWNAME,
    A.PRODUCTSPECNAME,
    A.PRODUCTSPECVERSION,
    A.CARRIERNAME,
    A.PRODUCTIONTYPE,
    A.PRIORITY,
    A.PROCESSOPERATIONNAME,
    A.PRODUCTTYPE,
    A.LOTPROCESSSTATE,
    A.LOTHOLDSTATE,
    A.PRODUCTQUANTITY,
    A.MACHINENAME,
    A.RELEASETIME,
    A.LASTLOGGEDOUTTIME,
    A.LASTLOGGEDINTIME,
    A.LASTEVENTTIME, 
	A.LOTSTATE,
    (SELECT ENUMNAME FROM ENUMDEFVALUE WHERE ENUMVALUE = A.OWNERID AND ENUMNAME LIKE 'Owner%' and rownum = 1) AS OWNER_TYPE,
    A.OWNERID AS OWNER_ID,    
    --ROUND((SYSDATE - A.LASTLOGGEDOUTTIME) * 24, 2) STAY_HOURS,
    NULL AS HOLD_TIME,
    A.SHEETNAME,
    NULL AS SHEET_ID_TIME,
    S.SHEETNAME AS INSP_SHEET_ID,
    S.LASTEVENTTIME AS INSP_SHEET_TIME, 
    --Q.REMAINTIME AS Q_TIME,  
    A.NODESTACK NODEID,     
    DECODE(PRE_OPER, NULL, PROCESSOPERATIONNAME, PRE_OPER) PRE_STEP_ID,
    DECODE(D.ONMACHINE, 'Y', 'Y', 'N') AS ONMACHINE
FROM 
    WIP A, DURABLE D, INSP_SH S, WIP_QUE_NEXT_OPER Q
 WHERE 1=1 
   AND A.LOTNAME = S.LOTNAME(+)
   AND A.LOTNAME = Q.LOTNAME(+)
   AND A.CARRIERNAME = D.DURABLENAME(+)</CommandText>
                    </cdd:DataCommand>
                  </Commands>
                </cdd:DataAction>
              </ActiveAction>
              <Actions>
                <cdd:DataAction ref:name="IF_LOT_Default" />
              </Actions>
            </cdd:DataItem>
