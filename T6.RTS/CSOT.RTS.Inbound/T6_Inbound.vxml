              </MetaInfo>
              <ActiveAction>
                <cdd:DataAction Name="Default" DataSource="T6RTS" IsAdapters="False" def:name="RTS_STEP_TIME_Default">
                  <Commands>
                    <cdd:DataCommand Name="Select" IsBindTable="True">
                      <CommandText>WITH STEP_TIME 
AS
(
    SELECT 
        FACTORY_ID, SHOP_ID, PROD_ID, PROC_ID, STEP_ID, EQP_ID,        
        MAX(TACT_TIME) AS TACT_TIME, 
        MAX(PROC_TIME) AS PROC_TIME, 
        MAX(LONG_TACT_TIME) AS LONG_TACT_TIME, 
        MAX(LONG_PROC_TIME) AS LONG_PROC_TIME,
        MAX(UPDATE_DTTM) AS UPDATE_DTTM, 
        MAX(DATA_FLAG) AS DATA_FLAG
    FROM
    (
        SELECT
            A.FACTORY_ID, A.SHOP_ID, A.PROD_ID,	A.PROC_ID, A.STEP_ID, A.EQP_ID,
            A.TACT_TIME, A.PROC_TIME, null AS LONG_TACT_TIME, null AS LONG_PROC_TIME,
            A.UPDATE_DTTM, A.DATA_FLAG
        FROM
            RTS_STEP_TIME A  
        WHERE 1=1
            AND (DATA_FLAG &lt;&gt; 'N' OR DATA_FLAG is null)
        UNION ALL
        SELECT
            A.FACTORY_ID, A.SHOP_ID, A.PROD_ID,	A.PROC_ID, A.STEP_ID, A.EQP_ID,
            null AS TACT_TIME, null AS PROC_TIME, A.TACT_TIME AS LONG_TACT_TIME, A.PROC_TIME AS LONG_PROC_TIME,
            A.UPDATE_DTTM, A.DATA_FLAG
        FROM
            RTS_STEP_TIME_LONG A
        WHERE 1=1
            AND (DATA_FLAG &lt;&gt; 'N' OR DATA_FLAG is null)
    )
    GROUP BY
        FACTORY_ID, SHOP_ID, PROD_ID, PROC_ID, STEP_ID, EQP_ID
)
SELECT * FROM
    STEP_TIME    
WHERE 1=1
	AND SHOP_ID IN($ARG_TARGET_SHOP_LIST) </CommandText>
                    </cdd:DataCommand>
                  </Commands>
                </cdd:DataAction>
              </ActiveAction>
