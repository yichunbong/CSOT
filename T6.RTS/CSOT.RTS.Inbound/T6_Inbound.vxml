                      <CommandText>WITH WIP
AS
(
SELECT LOTNAME FROM LOT A
WHERE 1=1
	AND A.LOTSTATE = 'Released'    
	AND A.PRODUCTIONTYPE = 'Production'
	AND A.FACTORYNAME IN ($ARG_TARGET_SHOP_LIST)  
)
SELECT     
    B.LOTNAME,    
    A.PRODUCTSPECNAME,
	A.FACTORYNAME,
    A.PROCESSOPERATIONNAME,    
    A.PROCESSFLOWNAME,    
    A.TOFACTORYNAME,
    A.TOPROCESSOPERATIONNAME,    
    A.TOPROCESSFLOWNAME,    
    MIN(A.FROMTRACKOUTTIME) AS FROMTRACKOUTTIME,
    MAX(A.MINQUEUETIME) AS MINQUEUETIME,
    MIN(A.MAXQUEUETIME) AS MAXQUEUETIME,
    A.STATE
FROM
    CT_PRODUCTQUEUETIME A,
    PRODUCT B
