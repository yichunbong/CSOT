        CT_PHOTORECIPETIME C, TPFOMPOLICY TP, POSMACHINERECIPE M
    WHERE 1=1
        AND C.MACHINENAME = TP.MACHINENAME           
        AND TP.CONDITIONID = M.CONDITIONID
