                    <cdd:DataCommand Name="Cmd1" IsBindTable="True">
                      <CommandText>--ARRAY/CF
SELECT 
    A.ORIENTEDSITE,
    A.FACTORYNAME,
    A.LOTNAME,
    A.PROCESSGROUPNAME,
    A.PROCESSFLOWNAME,
    A.PRODUCTSPECNAME,
    A.PRODUCTSPECVERSION,
    A.CARRIERNAME,
    A.PRODUCTIONTYPE,
    A.PRIORITY,
    A.PROCESSOPERATIONNAME,
    A.PRODUCTTYPE,
    A.LOTPROCESSSTATE,
    A.LOTHOLDSTATE,
    A.PRODUCTQUANTITY,
    A.MACHINENAME,
    A.RELEASETIME,
    A.LASTLOGGEDOUTTIME,
    A.LASTLOGGEDINTIME,
    A.LASTEVENTTIME,   
	A.LOTSTATE,
    (SELECT ENUMNAME FROM ENUMDEFVALUE WHERE ENUMVALUE = A.OWNERID AND ENUMNAME LIKE 'Owner%' and rownum = 1) AS OWNER_TYPE,
    A.OWNERID AS OWNER_ID,
    A.NODESTACK AS NODEID
FROM 
    LOT A, DURABLE D
 WHERE 1=1   
   AND A.CARRIERNAME = D.DURABLENAME(+)
   AND (A.PROCESSOPERATIONNAME = '0000' OR (A.LOTSTATE = 'Shipped' AND A.PROCESSOPERATIONNAME IN('9900', '9990'))) --BANK WIP
   AND A.PRODUCTIONTYPE = 'Production'
   AND A.FACTORYNAME IN ($ARG_TARGET_SHOP_LIST)
 /*
 UNION ALL
 --CELL BANK (HVA OUT ~ CUT WAIT, 2900 ~ 3000 WAIT)
 SELECT 
    A.ORIENTEDSITE,
    A.FACTORYNAME,
    A.LOTNAME,
    A.PROCESSGROUPNAME,
    A.PROCESSFLOWNAME,
    A.PRODUCTSPECNAME,
    A.PRODUCTSPECVERSION,
    A.CARRIERNAME,
    A.PRODUCTIONTYPE,
    A.PRIORITY,
    A.PROCESSOPERATIONNAME,
    A.PRODUCTTYPE,
    A.LOTPROCESSSTATE,
    A.LOTHOLDSTATE,
    A.PRODUCTQUANTITY,
    A.MACHINENAME,
    A.RELEASETIME,
    A.LASTLOGGEDOUTTIME,
    A.LASTLOGGEDINTIME,
    A.LASTEVENTTIME,   
	A.LOTSTATE,
    (SELECT ENUMNAME FROM ENUMDEFVALUE WHERE ENUMVALUE = A.OWNERID AND ENUMNAME LIKE 'Owner%' and rownum = 1) AS OWNER_TYPE,
    A.OWNERID AS OWNER_ID,
    A.NODESTACK AS NODEID
FROM 
    LOT A, DURABLE D
 WHERE 1=1   
   AND A.CARRIERNAME = D.DURABLENAME(+)
   AND (A.PROCESSOPERATIONNAME = '2900' OR (A.PROCESSOPERATIONNAME = '3000' AND A.LOTPROCESSSTATE &lt;&gt; 'RUN')) --BANK WIP
   AND A.PRODUCTIONTYPE = 'Production'
   AND A.FACTORYNAME IN ($ARG_TARGET_SHOP_LIST)
 */</CommandText>
                    </cdd:DataCommand>
                  </Commands>
                </cdd:DataAction>
              </ActiveAction>
