        SUPERMACHINENAME,
        DETAILMACHINETYPE
    FROM 
        MACHINESPEC 
    WHERE 1=1
        AND SUPERMACHINENAME IN (SELECT MACHINENAME FROM MACHINESPEC WHERE DETAILMACHINETYPE = 'MAIN')
