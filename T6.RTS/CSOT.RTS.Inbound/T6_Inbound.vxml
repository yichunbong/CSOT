            <cdd:DataItem Name="IF_MTPROJECT" Category="PMS">
              <MetaInfo>
                <cdm:DataMetaInfo Name="IF_MTPROJECT">
                  <Properties>
                    <cdm:PropertyMeta Name="FACTORYID" PropertyType="string" />
                    <cdm:PropertyMeta Name="DEPARTID" PropertyType="string" />
                    <cdm:PropertyMeta Name="MODELID" PropertyType="string" />
                    <cdm:PropertyMeta Name="EQID" PropertyType="string" />
                    <cdm:PropertyMeta Name="PROJECTSTATUS" PropertyType="int" />
                    <cdm:PropertyMeta Name="INITPLANTIME" PropertyType="DateTime" />
                    <cdm:PropertyMeta Name="FACTSTARTTIME" PropertyType="DateTime" />
                    <cdm:PropertyMeta Name="FACTENDTIME" PropertyType="DateTime" />
                    <cdm:PropertyMeta Name="FACTHOUR" PropertyType="float" />
                    <cdm:PropertyMeta Name="EXPECTSTARTTIME" PropertyType="DateTime" />
                    <cdm:PropertyMeta Name="EXPECTENDTIME" PropertyType="DateTime" />
                    <cdm:PropertyMeta Name="EXPECTHOUR" PropertyType="float" />
                    <cdm:PropertyMeta Name="DELAYDAYS" PropertyType="float" />
                    <cdm:PropertyMeta Name="PMSTARTTIME" PropertyType="DateTime" />
                    <cdm:PropertyMeta Name="PMENDTIME" PropertyType="DateTime" />
                    <cdm:PropertyMeta Name="PMHOUR" PropertyType="float" />
                    <cdm:PropertyMeta Name="UPDATETIME" PropertyType="DateTime" />
                  </Properties>
                </cdm:DataMetaInfo>
              </MetaInfo>
              <ActiveAction>
                <cdd:DataAction Name="Default" DataSource="T6PMS" IsAdapters="False" def:name="IF_MTPROJECT_Default">
                  <Commands>
                    <cdd:DataCommand Name="Cmd1">
                      <CommandText>#if (@ARG_RUN_SERVER == "DEV_LOCAL")
SELECT 
    NULL AS FACTORYID,
    NULL AS DEPARTID,
    NULL AS MODELID,
    NULL AS EQID,
    NULL AS PROJECTSTATUS,
	NULL AS INITPLANTIME,
    NULL AS FACTSTARTTIME,
    NULL AS FACTENDTIME,
    NULL AS FACTHOUR,
    NULL AS EXPECTSTARTTIME,
    NULL AS EXPECTENDTIME,
    NULL AS EXPECTHOUR,
    NULL AS PMSTARTTIME,
    NULL AS PMENDTIME,
    NULL AS PMHOUR,
    NULL AS DELAYDAYS,
    NULL AS UPDATETIME
FROM
    DUAL A
WHERE 1 &lt;&gt; 1    
#else
WITH PMSCHED
AS
(
    SELECT 
        B.FACTORYID,
        B.DEPARTID,
        B.MODELID,
        A.EQID,
        A.PROJECTSTATUS,
		A.INITPLANTIME,
        A.FACTSTARTTIME,
        A.FACTENDTIME,
        A.FACTHOUR,
        A.EXPECTSTARTTIME,
        A.EXPECTENDTIME,
        A.EXPECTHOUR,
        CASE WHEN A.PROJECTSTATUS IN (0, 1, 2, 3, 6, 9) THEN A.EXPECTSTARTTIME
        ELSE FACTSTARTTIME
        END PMSTARTTIME,
        CASE WHEN PROJECTSTATUS IN (0, 1, 2, 3, 6, 9) THEN A.EXPECTENDTIME
        ELSE FACTENDTIME
        END PMENDTIME,
        CASE WHEN PROJECTSTATUS IN (0, 1, 2, 3, 6, 9) THEN A.EXPECTHOUR
        ELSE FACTHOUR
        END PMHOUR,  
		C.DELAYDAYS,           
        A.UPDATETIME
    FROM
        MTPROJECT A,
        (
            SELECT
                BB.FACTORYID, AA.DEPARTID, AA.EQID, AA.MODELID
            FROM
                MTEQINFO AA, DEPART BB
            WHERE 1=1
                AND AA.DEPARTID = BB.DEPARTID			
                AND BB.FACTORYID = 'PM6'
        ) B,
        MTEQMODEL C
    WHERE 1=1
        AND A.EQID = B.EQID
        AND B.MODELID = C.MODELID
        AND A.PROJECTSTATUS IN (0, 1, 2, 3, 4, 5, 6, 9)
)
SELECT
    *
FROM
    PMSCHED
WHERE 1=1        
    AND 
	(
	   (PMSTARTTIME &gt;= TO_DATE(@ARG_PLAN_START_TIME, 'YYYYMMDDHH24MISS') - 15 AND PMENDTIME &lt; TO_DATE(@ARG_PLAN_START_TIME, 'YYYYMMDDHH24MISS') + 15)
	   OR (EXPECTSTARTTIME &gt;= TO_DATE(@ARG_PLAN_START_TIME, 'YYYYMMDDHH24MISS') - 2 AND EXPECTSTARTTIME &lt; TO_DATE(@ARG_PLAN_START_TIME, 'YYYYMMDDHH24MISS') + 2)
	)
#endif</CommandText>
                      <Parameters>
                        <cdd:DataParameter Name="@ARG_RUN_SERVER" SourceVersion="Current" />
                        <cdd:DataParameter Name="@ARG_PLAN_START_TIME" SourceVersion="Current" />
                        <cdd:DataParameter Name="@ARG_PLAN_END_TIME" SourceVersion="Current" />
                      </Parameters>
                    </cdd:DataCommand>
                  </Commands>
                </cdd:DataAction>
              </ActiveAction>
              <Actions>
                <cdd:DataAction ref:name="IF_MTPROJECT_Default" />
              </Actions>
