                      <Items Capacity="4">
                        <cdm:DataViewItem Name="View" IndexType="Hashtable">
                          <Keys Capacity="4">
                            <String>SHOP_ID</String>
                            <String>PRODUCT_ID</String>
                            <String>PROCESS_ID</String>
                            <String>STEP_ID</String>
                          </Keys>
                        </cdm:DataViewItem>
                        <cdm:DataViewItem Name="View2" IndexType="Hashtable">
                          <Keys Capacity="4">
                            <String>SHOP_ID</String>
                            <String>PRODUCT_ID</String>
                            <String>STEP_ID</String>
                          </Keys>
                        </cdm:DataViewItem>
                        <cdm:DataViewItem Name="View3" IndexType="Hashtable">
                          <Keys Capacity="4">
                            <String>STEP_ID</String>
                          </Keys>
                        </cdm:DataViewItem>
                        <cdm:DataViewItem Name="EqpView" IndexType="Hashtable">
                          <Keys Capacity="4">
                            <String>EQP_ID</String>
                          </Keys>
                        </cdm:DataViewItem>
                      </Items>
