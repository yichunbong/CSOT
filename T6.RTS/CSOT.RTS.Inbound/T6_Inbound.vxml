        INBOUND_START_TIME = @INBOUND_START_TIME,
        INBOUND_END_TIME = CASE WHEN TO_CHAR(@INBOUND_END_TIME, 'YYYY') = '0001' THEN null ELSE @INBOUND_END_TIME END,        
        DESCRIPTION = SUBSTRB(@DESCRIPTION, 1, 1000),
        UPDATE_TIME = SYSDATE --@UPDATE_TIME
