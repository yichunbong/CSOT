                  <Commands>
                    <cdd:DataCommand Name="Cmd1" IsBindTable="True">
                      <CommandText>SELECT * FROM
(
    SELECT 
        TP.FACTORYNAME,
        C.MACHINENAME,
        C.RECIPENAME,
        C.LOTNAME,
        (SELECT PRODUCTSPECNAME FROM LOT WHERE LOTNAME = C.LOTNAME ) AS PRODUCTSPECNAME,
        (SELECT PRODUCTSPECVERSION FROM LOT WHERE LOTNAME = C.LOTNAME) AS PRODUCTSPECVERSION,
        C.TKOUTPROCESSOPERATIONNAME AS PROCESSOPERATIONNAME,
        C.TKOUTDATETIME,
        C.TKOUTUSER,
        C.DUEDATETIME,
        C.CHECKFLAG,
        C.MAXCOUNT,
